{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true,
    "paths": {
      "@/*": ["src/render/*"],
      "@@/*": ["src/render/.umi/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/render/mock/**/*",
    "src/**/*",
    "config/**/*",
    "src/render/.umirc.ts"
  ]
}
